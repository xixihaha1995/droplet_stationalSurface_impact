FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    //- Set patchGroups for constraint patches
    // #includeEtc "caseDicts/setConstraintTypes"

    WALL2
    {
        type nutkWallFunction;
        value uniform 0;
    }

    WALL1
    {
        type nutkWallFunction;
        value uniform 0;

    }

    TOP
    {
        type calculated;
        value uniform 0;
    }

    WALL3
    {
        type nutkWallFunction;
        value uniform 0;
    }
    WALL4
    {
        type nutkWallFunction;
        value uniform 0;
    }
    BTM
    {
        type nutkWallFunction;
        value uniform 0;
    }
}


